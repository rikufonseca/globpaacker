<div class="container-fluid" style="padding-top: 20px;">
  <div class="title-card">
    <h1><%= @place.name %></h1>
 <li><%= @place.photo  %> </li>
    <li><strong>Duration: </strong><%= @place.duration %> minutes | <%= @place.exterior? ? "Exterior" : "Interior" %></li>
  <li><strong>Senses: </strong> <%= @place.senses.gsub('"', '').gsub('[', '').gsub(']', '') %> | <strong>Interests: </strong> <%= @place.interest.gsub('"', '').gsub('[', '').gsub(']', '') %></li>
  <li><strong>Environment: </strong> <%= @place.environment.gsub('"', '').gsub('[', '').gsub(']', '') %> | <strong>Rating: </strong> <%= @place.rating %> /10</li>
  <li><strong>To know everything: </strong> <%= @place.description %></li>
  </ul>

  </div>
